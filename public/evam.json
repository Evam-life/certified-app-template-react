{
  "applicationId": "com.example.certified-app-template",
  "applicationName": "certified-app-template",
  "version": "0.0.0",
  "versionCode": 1,
  "logo": "logo.png",
  "apiVersion": {
    "target": "1",
    "minimum": "1"
  },
  "permissions": [
    "ACTIVE_OPERATION_READ",
    "BATTERY_READ",
    "DEVICE_ROLE_READ",
    "CONNECTIVITY_READ",
    "LOCATION_READ",
    "OPERATION_READ",
    "TRIP_HISTORY_READ",
    "VEHICLE_STATE_READ",
    "SEND_NOTIFICATION",
    "DISPLAY_MODE_READ",
    "NAVIGATION_PRIVATE_LAYERS",
    "AVAILABLE_VEHICLE_STATUS_LIST_READ",
    "DEVICE_ROLE_READ",
    "ACTIVE_OPERATION_WRITE",
    "RAKEL_STATE_READ",
    "RAKEL_COMMUNICATION_READ",
    "RAKEL_RAW_COMMAND_SEND",
    "PUT_APP_IN_FOREGROUND"
